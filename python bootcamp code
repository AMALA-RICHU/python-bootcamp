
input1 = int(input("Enter the total numbers you wish to enter"))
print("enter each number")
print((input1))
input_list = []
output_list = []
for i in range((int(input1))):
    print("enter element:",i+1)
    input_list.append(input())
print("elements in the list",input_list)
for i in input_list:
    for j in input_list:
        if ((int(i) * int(j) )% 2 ==0 and (int(i) + int(j)) %2 != 0  ):
            output_list.append((i,j))

print("output",output_list)
